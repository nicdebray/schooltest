<%= simple_form_for @student do |f| %>
<%= f.input :first_name,
            autofocus: true,
            input_html: { autocomplete: "Student's first name",
              class: 'form-control form-margin-btn'} %>
<%= f.input :last_name,
            input_html: { autocomplete: "Student's last name",
              class: 'form-control form-margin-btn'} %>
<p>
<%= f.label :batch, class: "student_label_batch_name" %>
<%= collection_select :student, :batch_id, current_user.batches , :id, :name, { prompt: true } %>
</p>
<%= f.input :photo %>
<%= f.input :photo_cache, as: :hidden %>
<%= f.button :submit, class: "btn btn-success btn-form-edit-new" %>
<% end %>


