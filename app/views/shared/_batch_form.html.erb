<%= simple_form_for @batch do |f| %>
<%= f.input :name,
            required: true,
            autofocus: true,
            input_html: { autocomplete: "Batch's name",
            class: 'form-control form-margin-btn' } %>
<%= f.button :submit, class: "btn btn-success btn-form-edit-new" %>
<% end %>
