<%= simple_form_for @teacher do |f| %>
<%= f.input :first_name,
            autofocus: true,
            input_html: { autocomplete: "Teacher's first name",
            class: 'form-control form-margin-btn' } %>
<%= f.input :last_name,
            input_html: { autocomplete: "Teacher's last name",
            class: 'form-control form-margin-btn' } %>
<%= f.input :photo %>
<%= f.input :photo_cache, as: :hidden %>
<%= f.button :submit, class: "btn btn-success btn-form-edit-new" %>
<% end %>

