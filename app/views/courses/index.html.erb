<h1>Courses#index</h1>
<p>Find me in app/views/courses/index.html.erb</p>
<%= link_to "New Course", courses_new_path %>
<% @courses.each do |course| %>
<ul>
  <li><%= course.name %></li>
  <li><%= link_to "Delete", course_path(course), method: :delete %></li>
  <li><%= link_to "Edit", edit_course_path(course) %></li>
</ul>
<% end %>
